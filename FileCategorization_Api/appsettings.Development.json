{
  "ConnectionStrings": {
    "sqliteConnection": "Data Source=Temp/FileCat.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Temp/FC/FC.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3} {Username} {Message:lj}{Exception}{NewLine}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "TokenExpirationMinutes": "15",
  "deployLogPath": "Temp/DeployLogs/",
  "JWT": {
    "ValidAudience": "https://localhost:7014",
    "ValidIssuer": "https://localhost:7014",
    "Secret": "your-secret-key-here-make-it-long-enough"
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 10485760,
      "MaxConcurrentConnections": 50,
      "MaxConcurrentUpgradedConnections": 50
    }
  }
}
